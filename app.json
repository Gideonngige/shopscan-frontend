{
  "expo": {
    "name": "ShopScan",
    "slug": "ShopScan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images2/ShopScan.png",
    "scheme": "shopscan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images2/ShopScan.png",
        "backgroundImage": "./assets/images2/ShopScan.png",
        "monochromeImage": "./assets/images2/ShopScan.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gideon_ushindi.ShopScan"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images2/ShopScan.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images2/ShopScan.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9577a63d-3b92-43a9-bec4-48bf87a55143"
      }
    },
    "owner": "gideon_ushindi"
  }
}
